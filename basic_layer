<script>
/* 기본함수 */
// 1. scroller : 스크롤 이동 함수 (이동할 Dom의 id와, Dom 객체로부터 여유를 줄 margin-top 값을 반영하여 이동)
// 2. popupControll : 모달창 제어 함수
    function getOffsetTop(el) {
        var top = -30;
        if (el.offsetParent) {
            do { top += el.offsetTop; } while (el = el.offsetParent); return [top];
        }
    }
    function goTo(id){
        window.scroll(0, getOffsetTop(document.getElementById(id)));
    }
    function scroller(id,location){
        let scrollTo = $('#'+id).offset().top;
        scrollTo = scrollTo - location;
        if(id == 'content_m' || id == 'content_w'){scrollTo = 0;}
        $('html, body').animate({scrollTop:scrollTo},500);
    }
    function popupControll(id,action){
        if(action == 'open'){
            $('body').css({'height':'100vh'});
            $('.popup_wrapper').css({'display':'flex'});
            $('#'+id).css({'display':'flex'});
        }else if(action == 'close'){
            $('.popup_wrapper').hide();
            $('#'+id).hide();
        }else{
            console.log('팝업 제어 타입이 지정되지 않았습니다');
        }
    }
</script>
<script type="text/javascript">
/*페이지 공유*/
    var og_title = '';
    var og_image = '';

    $(document).ready(function(){
        var href = window.location.href;
        var apppath = location.pathname + location.search;
        console.log(apppath)

        Kakao.Link.createDefaultButton({
            container:  '#kakao-link-btn',
            objectType: 'feed',
            content:{
                title: $("meta[name='og:title']")[0]? $('meta[name="og:title"]').attr('content') : og_title,
                imageUrl: $("meta[name='og:image']")[0]? $('meta[name="og:image"]').attr('content') : og_image,
                link: { mobileWebUrl:href, webUrl:href }
            },
            buttons:[{
                title: '웹으로 보기',
                link: { mobileWebUrl:href, webUrl:href }
            },
            // 도메인 주소가 신규로 추가된 경우, 메인 도메인주소가 달라서 앱으로 연결이 안된다.
            // 현재 앱에서 모두 연결을 https://hellonature.net/ + apppath 이동하기 때문에 현재 카카오톡애서 앱을 열 방법이 없다.
            // {
            //     title: '앱으로 보기',
            //     link: { androidExecParams: apppath, iosExecParams: apppath }
            // }
            ],
        });

        $("button[data-toggle='sns_share']").click(function(e){
            var _this = $(this);
            var sns_type = _this.attr('data-service');
            var href = window.location.href;
            var title = '페이지 공유하기';
            var loc = "";
            var img = $("meta[name='og:image']").attr('content');


            if( ! sns_type || !href || !title) return;

            switch (sns_type) {
                case 'kakao':
                    break;
                case 'facebook':
                    loc = '//www.facebook.com/sharer/sharer.php?u='+href+'&t='+title;
                    window.open(loc);
                    break;
                case 'kakaostory':
                    loc = 'https://story.kakao.com/share?url='+encodeURIComponent(href);
                    window.open(loc);
                    break;
                case 'blog':
                    loc = "https://share.naver.com/web/shareView.nhn?url="+encodeURIComponent(href)+"&title="+encodeURIComponent(title);
                    window.open(loc);
                    break;
                case 'sms':
                    var userAgent = navigator.userAgent.toLowerCase();
                    var isAndroid = userAgent.match('android') != null;
                    var isiOS = userAgent.indexOf("iphone") >-1 || userAgent.indexOf("ipad") >-1 || userAgent.indexOf("ipod") >-1;
                    loc = isAndroid ? 'sms:?body='+href+ " " + title : 'sms:&body='+href + " " + title ;
                    location.href = loc;
                break;
                case 'url':
                    e.preventDefault();
                    var field = document.getElementById('inst_copy_url');
                    field.value = href;
                    field.select();
                    field.setSelectionRange(0, field.value.length)
                    var copysuccess = copySelectionText()
                    if (copysuccess) {
                        alert('복사 되었습니다.');
                        popupControll('popup_share','close');
                    }else{
                        alert('URL 복사에 실패했습니다.');
                    }

                    function copySelectionText(){
                        var copysuccess;
                        try{
                            copysuccess = document.execCommand("copy");
                        } catch(e){
                            copysuccess = false;
                        }
                        return copysuccess;
                    }
                    break;
                default:
                console.log('바르지 않는 접근입니다');
                return false;
            }
            return false;
        });
    });
</script>
